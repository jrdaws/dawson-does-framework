{
  "frameworkName": "Dawson Does Framework",
  "projectDefaults": {
    "outputDir": "tasks",
    "assumptionPolicy": "State assumptions explicitly. If missing info blocks action, propose safe defaults and proceed.",
    "decisionStyle": "Bias toward shipping, keep scope aligned with PROJECT_SIZE."
  },
  "models": {
    "research": { "provider": "anthropic", "model": "claude-3-5-sonnet-latest" },
    "executor": { "provider": "openai", "model": "gpt-5.2-thinking" },
    "reviewer": { "provider": "openai", "model": "gpt-5.2-thinking" }
  },
  "commands": {
    "start": {
      "phases": ["research", "executor", "reviewer"]
    },
    "follow-rules": {
      "outputFile": "RULES_AUDIT.md"
    },
    "compacting": {
      "outputFile": "HANDOFF.md"
    }
  }
}
