{
  "id": "saas",
  "name": "SaaS Starter",
  "version": "1.1.0",
  "description": "Full-stack SaaS template with authentication, billing, and database. Perfect for launching subscription-based web applications.",
  "author": "Dawson Framework Team",
  "category": "SaaS",
  "tags": ["nextjs", "react", "saas", "subscription", "auth", "billing"],
  "minFrameworkVersion": "0.1.0",
  "capabilities": ["auth.supabase", "billing.stripe"],
  "dependencies": {
    "next": "^15.1.6",
    "react": "^19.0.0",
    "@supabase/supabase-js": "^2.47.0"
  },
  "features": [
    "Next.js 15 with App Router",
    "Supabase authentication",
    "Stripe billing integration",
    "TypeScript support",
    "Tailwind CSS",
    "Server components",
    "Optimized media assets"
  ],
  "assets": {
    "images": [
      {
        "id": "hero-workspace",
        "path": "public/images/hero-workspace.webp",
        "dimensions": "1920x1080",
        "category": "hero",
        "description": "Desktop hero image - analytics dashboard workspace"
      },
      {
        "id": "hero-workspace-mobile",
        "path": "public/images/hero-workspace-mobile.webp",
        "dimensions": "750x1000",
        "category": "hero",
        "description": "Mobile hero image variant"
      },
      {
        "id": "empty-state-data",
        "path": "public/images/empty-state-data.webp",
        "dimensions": "400x300",
        "category": "empty-states",
        "description": "Empty data state illustration"
      }
    ],
    "icons": [
      {
        "id": "icon-analytics",
        "path": "public/images/icon-analytics.svg",
        "dimensions": "64x64",
        "description": "Analytics icon - outline style"
      },
      {
        "id": "icon-analytics-2x",
        "path": "public/images/icon-analytics-2x.svg",
        "dimensions": "128x128",
        "description": "Analytics icon retina"
      }
    ]
  },
  "supportedIntegrations": {
    "auth": ["supabase", "clerk"],
    "payments": ["stripe", "paddle"],
    "email": ["resend", "sendgrid"],
    "db": ["supabase", "planetscale"],
    "ai": ["openai", "anthropic"],
    "analytics": ["posthog", "plausible"]
  },
  "defaultIntegrations": {
    "auth": "supabase",
    "payments": "stripe",
    "db": "supabase"
  },
  "requiredIntegrations": ["auth", "db"],
  "license": "MIT"
}
